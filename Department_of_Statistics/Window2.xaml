<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" xmlns:chartingToolkit="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit" x:Class="Department_of_Statistics.Window2"
        mc:Ignorable="d"
        Title="Window2" Height="450" Width="800">
    <Grid>
        <chartingToolkit:Chart  Name="pieChart" Title="Departments and their Municipalities" VerticalAlignment="Top" Margin="32,47,46,0" Height="326">
            <chartingToolkit:PieSeries DependentValuePath="Value" IndependentValuePath="Key" ItemsSource="{Binding}" IsSelectionEnabled="True" />
        </chartingToolkit:Chart>
    </Grid>
</Window>
